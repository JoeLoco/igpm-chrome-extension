{
    "manifest_version": 2,
    "name": "IGP-M Fácil",
    "description": "Calculadora de IGP-M (Índice Geral de Preços do Mercado)",
    "version": "1.1",
    "browser_action": {
        "default_icon": "128x128.png",
        "default_popup": "app.html"
    },
    "icons": {
        "16": "16x16.png",
        "64": "64x64.png",
        "128": "128x128.png"
    }
    ,
    "permissions": [
        "activeTab",
        "https://ajax.googleapis.com/"
    ]
}
